#include<stdio.h>
int result,a,b;
int multiply(int a,int b)
{
    printf("enter a");
  scanf("%d",&a);

    printf("enter b");
    scanf("%d",&b);
    
   
    result=a*b;
    return result;
    

}
void main()
{int x,y,c,res;
printf("enter x");;
scanf("%d",&x);
printf("enter y");
 scanf("%d",&y);
 c=x+y;
  res=multiply(a,b)+c;
